<form class="form-inline" method="POST" action="/addUser" class="form-inline">
	<input name="name" type="text" class="input" placeholder="name" autocomplete="off">
	<br />
	<br />
	<input name="password" type="password" class="input" placeholder="password">
	<br />
	<br />
	<input name="type" type="radio" class="input" value="admin" checked="checked"> Admin 
	<input name="type" type="radio" class="input" value="employee"> Employee 
	<input name="type" type="radio" class="input" value="client"> Client
	<br />
	<br />
	<button type="submit" class="btn">Create User</button>
</form>
